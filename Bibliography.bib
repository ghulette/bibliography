	Isbn = {0-7695-3064-8},
	Keywords = {workflow},
	Pages = {35-42},
	Publisher = {IEEE Computer Society},
	Read = {Yes},
	Title = {A Dynamic Critical Path Algorithm for Scheduling Scientific Workflow Applications on Global Grids},
	Year = {2007},
